{
  "projects": [
    {
      "name": "frontend",
      "root": "frontend",
      "builds": [
        {
          "src": "frontend/package.json",
          "use": "@vercel/next"
        }
      ],
      "routes": [{ "src": "/(.*)", "dest": "frontend/$1" }]
    },
    {
      "name": "dashboard",
      "root": "dashboard",
      "builds": [
        {
          "src": "dashboard/package.json",
          "use": "@vercel/next"
        }
      ],
      "routes": [{ "src": "/(.*)", "dest": "dashboard/$1" }]
    }
  ]
}
